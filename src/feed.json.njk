[
{%- for post in collections.featured | reverse -%}
  {
    "title": {{ post.data.title | dump }},
    "url": "https://courtnotes.netlify.app{{ post.url }}",
    "date": "{{ post.date | postDate }}",
    "description": {{ post.data.description | dump }},
    "image": "https://courtnotes.netlify.app{{ post.data.image | url }}",
    "imageAlt": {{ post.data.imageAlt | dump }}
  }{%- if not loop.last -%},{%- endif -%}
{%- endfor -%}
]
